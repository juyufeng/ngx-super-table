{
  "name": "ngx-super-table",
  "description": "A table component for angular projects (angular versions >=2)",
  "homepage": "https://github.com/andyperlitch/ngx-super-table#readme",
  "bugs": {
    "url": "https://github.com/andyperlitch/ngx-super-table/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andyperlitch/ngx-super-table.git"
  },
  "license": "MIT",
  "author": "Andy Perlitch",
  "version": "0.0.0",
  "angular-cli": {},
  "scripts": {
    "commit": "git-cz",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:dist": "./scripts/build.sh",
    "build:demo": "ng build --base-href=/ngx-super-table/demo/ --output-path=demo",
    "build:clean": "rimraf build dist waste deploy",
    "prepublish": "npm run build:clean",
    "publish": "git push origin master && git push --tags origin master && npm run build:dist && ./scripts/publish.sh",
    "postpublish": "npm run gh-pages",
    "test": "ng test --watch false",
    "test:ci": "ng test --browser ChromeHeadless --watch false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "e2e:ci": "ng e2e --config protractor-ci.conf.js --watch false",
    "gh-pages": "git checkout gh-pages && git merge master && npm run build:demo && npm run typedoc && git add . && git commit -m 'chore: build demo and docs' && git push && git checkout master",
    "prerelease": "npm test",
    "release": "cd src/lib && standard-version",
    "typedoc": "typedoc --options typedoc.json ngx-super-table.ts src/*.ts",
    "posttypedoc": "./scripts/posttypedoc.sh"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.0.0",
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "bootstrap": "^4.0.0-alpha.6",
    "core-js": "^2.4.1",
    "lodash": "^4.17.0",
    "rxjs": "^5.4.2",
    "zone.js": "^0.8.17"
  },
  "devDependencies": {
    "@angular/cli": "^1.3.2",
    "@angular/compiler-cli": "^4.0.0",
    "@angular/language-service": "^4.0.0",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/lodash": "^4.14.71",
    "@types/node": "~6.0.60",
    "all-contributors-cli": "^4.4.0",
    "codelyzer": "~3.0.1",
    "commitizen": "~2.8.1",
    "conventional-changelog": "~1.1.0",
    "conventional-changelog-cli": "~1.2.0",
    "cz-conventional-changelog": "~1.2.0",
    "ghooks": "~1.3.2",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "rimraf": "^2.6.1",
    "rollup": "^0.45.2",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "standard-version": "^4.2.0",
    "ts-node": "~3.0.4",
    "tslint": "~5.3.2",
    "typedoc": "^0.8.0",
    "typescript": "~2.3.3",
    "validate-commit-msg": "~2.8.0"
  },
  "config": {
    "ghooks": {
      "commit-msg": "validate-commit-msg"
    },
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
